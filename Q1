/*Create a program that takes user input as a floating-point number, 
converts it to an integer using typecasting, 
and displays both the original and the typecasted values.*/

#include<bits/stdc++.h>
using namespace std;

int main() {
    float a;
    int b;
    cin>>a;
    b=static_cast<int>(a);
    cout<<"Original floating point number is "<<a<<endl;
    cout<<"Type casted interger value is "<<b<<endl;
	
	return 0;
}
