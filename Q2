/*Develop a program that calculates the area of a circle using a constant value for pi (Ï€). Use appropriate constants for radius and pi.*/
#include <iostream>
using namespace std;
int main(){
    float r;
    cin>>r;
    float pi=3.14;
    float area= pi*r*r;
    cout<<"Area of circle with radius "<<r<<" is "<<area<<endl; 
    return 0;
}
